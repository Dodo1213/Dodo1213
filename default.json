{
   "$schema":"https://docs.renovatebot.com/renovate-schema.json",
   "baseBranches":[
      "development"
   ],
   "dependencyDashboard":true,
   "labels":[
      "dependencies"
   ],
   "branchPrefix":"deps/",
   "extends":[
      "config:base"
   ],
   "reviewers":[
      "Dodo1213"
   ],
   "prConcurrentLimit":3,
   "prHourlyLimit":10,
   "packageRules":[
      {
         "matchDepNames":[
            "gradle",
            "maven",
            "com.diffplug.spotless",
            "org.gradle.toolchains.foojay-resolver-convention",
            "de.chojo.publishdata",
            "actions/checkout",
            "actions/setup-java",
            "docker/build-push-action",
            "docker/login-action",
            "gradle/wrapper-validation-action",
            "com.bucket4j:bucket4j-core",
            "org.apache.commons:commons-text",
            "org.jetbrains:annotations",
            "org.mockito:mockito-core",
            "org.postgresql:postgresql",
            "@types/express",
            "@types/node",
            "@types/pg",
            "@types/node-cron",
            "@types/uuid",
            "typescript",
            "ts-node",
            "express",
            "pg",
            "node-cron",
            "dotenv",
            "uuid",
            "actions/checkout",
            "actions/setup-node",
            "docker/build-push-action",
            "docker/login-action"
         ],
         "automerge":true,
         "squash":true
      }
   ]
}
